{
  "name": "cypress-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for cypress project",
  "author": "JÃ³zef Szymala",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:start": "npm run reports:clean; npm run cy:run; npm run reports:merge; npm run reports:generate",
    "reports:clean": "rm -rf cypress/reports/integration",
    "reports:merge": "npx mochawesome-merge cypress/reports/integration/*.json --output cypress/reports/report.json",
    "reports:generate": "npx mochawesome-report-generator --reportDir cypress/reports cypress/reports/report.json",
    "lint": "eslint  \"cypress/**/*.js\" --fix --ignore-path=\".gitignore\"",
    "prettier": "prettier --write \"cypress/**/*.js\" --ignore-path=\".gitignore\""
  },
  "dependencies": {
    "cypress": "5.1.0",
    "cypress-file-upload": "4.1.1",
    "cypress-plugin-tab": "1.0.5",
    "cypress-xpath": "1.4.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "faker": "4.1.0",
    "fs-extra": "9.0.0",
    "husky": "4.2.3",
    "lint-staged": "10.1.0",
    "mocha": "8.1.1",
    "mochawesome": "6.1.1",
    "mochawesome-merge": "4.1.0",
    "mochawesome-report-generator": "5.1.0",
    "prettier": "2.0.2"
  },
  "lint-staged": {
    "cypress/**/*.js": [
      "npm run prettier",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
