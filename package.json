{
  "name": "cypress-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for cypress project",
  "author": "JÃ³zef Szymala",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "e2e:develop": "cypress run --env configFile=develop --headed",
    "e2e:local": "cypress run --env configFile=local --headed",
    "e2e:staging": "cypress run --env configFile=staging --headed",
    "lint": "eslint  \"cypress/**/*.js\" --fix --ignore-path=\".gitignore\"",
    "prettier": "prettier --write \"cypress/**/*.js\" --ignore-path=\".gitignore\""
  },
  "dependencies": {
    "cypress": "4.3.0",
    "cypress-xpath": "1.4.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "fs-extra": "9.0.0",
    "husky": "4.2.3",
    "lint-staged": "10.1.0",
    "prettier": "2.0.2"
  },
  "lint-staged": {
    "cypress/**/*.js": [
      "npm run prettier",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
